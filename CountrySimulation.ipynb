   "source": [
    "# Reward Functions"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "190c5d04",
   "metadata": {},
   "outputs": [],
   "source": [
    "def undiscounted_reward(node, my_country):\n",
    "    root_node_score = maslowHeuristicVal(maslow(root_country_df, my_country, 5))\n",
    "    cur_node_score = maslowHeuristicVal(maslow(node.getCountrydf(), my_country, 5))\n",
    "    return cur_node_score - root_node_score"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3b967011",
   "metadata": {},
   "outputs": [],
   "source": [
    "GAMMA = 0.05\n",
    "def discounted_reward(node, my_country):\n",
    "    return (GAMMA ** len(node.getSched()))*undiscounted_reward(node, my_country)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "6b9a710c",
   "metadata": {},
   "outputs": [],
   "source": [
    "COST_FAILURE = -1\n",
    "PROB = 0.95\n",
    "def expected_utility(node, my_country):\n",
    "    success = PROB * discounted_reward(node, my_country)\n",
    "    fail = (1 - PROB) * COST_FAILURE\n",
    "    return success + fail"
   ]
  },
